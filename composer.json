{
    "name": "noouh/auto-model",
    "description": "A Laravel package to auto-generate models and migrations from table definitions.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Eng. Mohamed Hekal",
            "email": "info@noouh.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Noouh\\AutoModel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Noouh\\AutoModel\\AutoModelServiceProvider"
            ]
        }
    }
}